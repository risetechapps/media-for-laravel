{
    "name": "risetechapps/media-for-laravel",
    "description": "Laravel package for advanced media and upload management.",
    "keywords": [
        "risetechapps",
        "laravel",
        "media",
        "uploads",
        "spatie"
    ],
    "homepage": "https://github.com/risetechapps/media-for-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rise Tech",
            "email": "apps@risetech.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "php-ffmpeg/php-ffmpeg": "^1.3",
        "risetechapps/has-uuid-for-laravel": "^1.0",
        "risetechapps/monitoring-for-laravel": "^1.0",
        "spatie/laravel-medialibrary": "^11.17",
        "spatie/pdf-to-image": "^1.2",
        "tpetry/laravel-postgresql-enhanced": "^3.5",
        "risetechapps/risetools": "1.7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2",
        "laravel/pint": "^1.16",
        "laravel/sail": "^1.29",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "RiseTechApps\\Media\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiseTechApps\\Media\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RiseTechApps\\Media\\MediaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.1.0"
}
