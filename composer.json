{
    "name": "risetechapps/media-for-laravel",
    "description": "",
    "keywords": [
        "risetechapps",
        "media",
        "uploads"
    ],
    "homepage": "https://github.com/risetechapps/media-for-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rise Tech",
            "email": "apps@risetech.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12",
        "league/flysystem-aws-s3-v3": "^3.0",
        "php-ffmpeg/php-ffmpeg": "^1.3",
        "risetechapps/has-uuid-for-laravel": "^1.0",
        "risetechapps/monitoring-for-laravel": "^1.0.0",
        "spatie/laravel-medialibrary": "11.17.5",
        "spatie/pdf-to-image": "^1.2",
        "tpetry/laravel-postgresql-enhanced": "^3.5.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "RiseTechApps\\Media\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiseTechApps\\Media\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RiseTechApps\\Media\\MediaServiceProvider"
            ],
            "aliases": {
                "Media": "RiseTechApps\\Media\\MediaFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.0"
}
